    "import networkx as nx\n",
    "\n",
    "def build_signed_digraph(df: pd.DataFrame) -> nx.DiGraph:\n",
    "    G = nx.DiGraph()\n",
    "    for _, r in df.iterrows():\n",
    "        src = str(r[\"source_genesymbol\"])\n",
    "        tgt = str(r[\"target_genesymbol\"])\n",
    "        sgn = int(r[\"sign\"])\n",
    "        G.add_edge(src, tgt, sign=sgn)\n",
    "    return G\n",
    "\n",
    "G_tcga_sel = build_signed_digraph(tcga_sel_df)\n",
    "G_geo_sel  = build_signed_digraph(geo_sel_df)\n",
    "\n",
    "G_tcga_ind = build_signed_digraph(tcga_ind_df)\n",
    "G_geo_ind  = build_signed_digraph(geo_ind_df)\n",
    "\n",
    "print(\"TCGA selected nodes/edges:\", G_tcga_sel.number_of_nodes(), G_tcga_sel.number_of_edges())\n",
    "print(\"GEO  selected nodes/edges:\", G_geo_sel.number_of_nodes(),  G_geo_sel.number_of_edges())\n",
    "print(\"TCGA induced nodes/edges :\", G_tcga_ind.number_of_nodes(), G_tcga_ind.number_of_edges())\n",
    "print(\"GEO  induced nodes/edges :\", G_geo_ind.number_of_nodes(),  G_geo_ind.number_of_edges())\n"
   ]
