    "import pandas as pd\n",
    "\n",
    "def hub_table(G, top_n=20) -> pd.DataFrame:\n",
    "    nodes = sorted(G.nodes())\n",
    "    out_deg = dict(G.out_degree())\n",
    "    in_deg  = dict(G.in_degree())\n",
    "\n",
    "    df = pd.DataFrame({\n",
    "        \"node\": nodes,\n",
    "        \"out_degree\": [out_deg.get(n, 0) for n in nodes],\n",
    "        \"in_degree\":  [in_deg.get(n, 0) for n in nodes],\n",
    "    })\n",
    "    df[\"total_degree\"] = df[\"out_degree\"] + df[\"in_degree\"]\n",
    "    return df.sort_values(\"total_degree\", ascending=False).head(top_n)\n"
   ]
  },
