   "source": [
    "import matplotlib.pyplot as plt\n",
    "import networkx as nx\n",
    "\n",
    "def plot_network_signed(G, title, outpath, label_top_n=15, seed=42):\n",
    "    plt.figure(figsize=(12, 9))\n",
    "    pos = nx.spring_layout(G, seed=seed, k=0.9)\n",
    "\n",
    "    # nodes\n",
    "    nx.draw_networkx_nodes(G, pos, node_size=450)\n",
    "\n",
    "    # split edges by sign\n",
    "    act_edges = [(u, v) for u, v, d in G.edges(data=True) if int(d.get(\"sign\", 0)) == 1]\n",
    "    inh_edges = [(u, v) for u, v, d in G.edges(data=True) if int(d.get(\"sign\", 0)) == -1]\n",
    "\n",
    "    # draw activation: solid lines\n",
    "    nx.draw_networkx_edges(\n",
    "        G, pos,\n",
    "        edgelist=act_edges,\n",
    "        arrows=True,\n",
    "        style=\"solid\",\n",
    "        alpha=0.65,\n",
    "        width=1.3\n",
    "    )\n",
    "\n",
    "    # draw inhibition: dashed lines\n",
    "    nx.draw_networkx_edges(\n",
    "        G, pos,\n",
    "        edgelist=inh_edges,\n",
    "        arrows=True,\n",
    "        style=\"dashed\",\n",
    "        alpha=0.65,\n",
    "        width=1.3\n",
    "    )\n",
    "\n",
    "    # label nodes (all if small, otherwise top hubs)\n",
    "    if G.number_of_nodes() <= 30:\n",
    "        label_nodes = list(G.nodes())\n",
    "    else:\n",
    "        label_nodes = hub_table(G, top_n=label_top_n)[\"node\"].tolist()\n",
    "\n",
    "    nx.draw_networkx_labels(G, pos, labels={n: n for n in label_nodes}, font_size=10)\n",
    "\n",
    "    # legend \n",
    "    from matplotlib.lines import Line2D\n",
    "    legend_lines = [\n",
    "        Line2D([0], [0], linestyle=\"solid\", label=\"activation (+1)\"),\n",
    "        Line2D([0], [0], linestyle=\"dashed\", label=\"inhibition (−1)\")\n",
    "    ]\n",
    "    plt.legend(handles=legend_lines, loc=\"upper right\", frameon=True)\n",
    "\n",
    "    plt.title(title)\n",
    "    plt.axis(\"off\")\n",
    "    plt.tight_layout()\n",
    "    plt.savefig(outpath, dpi=300)\n",
    "    plt.close()\n",
    "plot_network_signed(G_tcga_sel, \"TCGA EMT network (selected edges)\", FIGS/\"tcga_selected_network.png\")\n",
    "plot_network_signed(G_geo_sel,  \"GEO EMT network (selected edges)\",  FIGS/\"geo_selected_network.png\")\n",
    "plot_network_signed(G_core_sel, \"Core EMT network (shared selected edges)\", FIGS/\"core_selected_network.png\")\n",
    "\n",
    "print(\"Saved signed-network figures in:\", FIGS)\n"
   ]
  },
