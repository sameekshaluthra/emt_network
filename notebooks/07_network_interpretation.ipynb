    "def edge_set(df: pd.DataFrame):\n",
    "    return set(zip(df[\"source_genesymbol\"], df[\"sign\"], df[\"target_genesymbol\"]))\n",
    "\n",
    "tcga_sel_edges = edge_set(tcga_sel_df)\n",
    "geo_sel_edges  = edge_set(geo_sel_df)\n",
    "\n",
    "shared_sel_edges = tcga_sel_edges & geo_sel_edges\n",
    "all_sel_edges = tcga_sel_edges | geo_sel_edges\n",
    "\n",
    "print(\"Selected edges overlap:\")\n",
    "print(\"TCGA:\", len(tcga_sel_edges))\n",
    "print(\"GEO :\", len(geo_sel_edges))\n",
    "print(\"Shared:\", len(shared_sel_edges))\n",
    "print(\"Overlap %:\", round(100 * len(shared_sel_edges) / len(all_sel_edges), 2))\n",
    "\n",
    "overlap_sel_df = pd.DataFrame(list(shared_sel_edges), columns=[\"source_genesymbol\", \"sign\", \"target_genesymbol\"])\n",
    "overlap_sel_df = overlap_sel_df.sort_values([\"source_genesymbol\",\"target_genesymbol\"]).reset_index(drop=True)\n",
    "\n",
    "OVERLAP_SEL_FILE = RESULTS / \"overlap_core_selected_edges.tsv\"\n",
    "overlap_sel_df.to_csv(OVERLAP_SEL_FILE, sep=\"\\t\", index=False)\n",
    "print(\"Saved:\", OVERLAP_SEL_FILE, \"shape:\", overlap_sel_df.shape)\n",
    "\n",
    "G_core_sel = build_signed_digraph(overlap_sel_df)\n",
    "print(\"Core selected nodes/edges:\", G_core_sel.number_of_nodes(), G_core_sel.number_of_edges())\n"
   ]
